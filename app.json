{
    "expo": {
        "name": "TitiPoint",
        "slug": "titipoint",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "expo": {
            "plugins": [
                [
                    "expo-image-picker",
                    {
                        "photosPermission": "Necesitamos tu permiso para poder acceder a las fotos."
                    }
                ]
            ]
        },
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "com.yassermussa.titipoint",
            "versionCode": 1,
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "description": ""
    }
}
